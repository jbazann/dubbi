---
import CommandTrigger from '@c/main-layout/CommandTrigger.astro'
import SitemapItem from '@c/SitemapItem.astro'
import Tab from '@c/utils/Tab.astro'

export const partial = true
export const prerender = false
const sitemap = {
    "/": {
        "/about": null,
        "/projects": {
            "/project1": null,
            "/project2": null,
            "/project3": null,
            "/project4": null,
        },
        "/gallery": null
    }
}
---

<ul>
    <SitemapItem path="/" depth={0} />
    <ul>
        <SitemapItem path="/about" depth={1} />
        <SitemapItem path="/projects" depth={1} />
        <ul>
            <SitemapItem path="/projects/project1" depth={2} />
            <SitemapItem path="/projects/project2" depth={2} />
            <SitemapItem path="/projects/project3" depth={2} />
            <SitemapItem path="/projects/project4" depth={2} />
        </ul>
        <SitemapItem path="/gallery" depth={1} />
    </ul>
</ul>